  "version": "0.0.5",
  "description": "Tools for TypeScript",
  "main": "index.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marviobezerra/xcommon.js.git"
  },
  "bugs": {
    "url": "https://github.com/marviobezerra/xcommon.js/issues"
  },
  "homepage": "https://github.com/marviobezerra/xcommon.js#readme",
  "keywords": [
    "xcommon",
    "layout"
  ],
  "contributors": [
    {
      "name": "Márvio André Bezerra Silverio",
      "email": "marvio.bezerra@gmail.com",
      "url": "http://www.marvio.com.br"
    }
  ],
  "scripts": {
    "clean": "rimraf wwwroot && rimraf lib",
    "build:dev": "npm run clean && webpack-dev-server --progress --colors --hot --inline --open",
    "build:pro": "npm run clean && tsc -p ./tsconfig.prod.json && npm run style",
    "style": "npm run style:sass && npm run style:css && npm run style:css:post",
    "style:sass": "copyfiles -f ./src/style/*.scss ./lib/style",
    "style:css": "node-sass --include-path scss ./src/style/layout-full.scss ./lib/style/layout.css",
    "style:css:post": "postcss ./lib/style/layout.css -o ./lib/style/layout.css -u autoprefixer --no-map"
  },
  "dependencies": {
    "@angular/animations": "^4.1.0",
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/platform-server": "^4.1.0",
    "@angular/router": "^4.1.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "ie-shim": "^0.1.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.1.0",
    "@types/core-js": "^0.9.41",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.14",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.0",
    "css-to-string-loader": "^0.1.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "postcss-cli": "^3.2.0",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.16.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.1.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  }
}
